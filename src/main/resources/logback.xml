<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="stdout" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%d{HH:mm:ss.SSS} [%thread] %-5level %logger{36} - %msg%n
			</pattern>
		</encoder>
	</appender>
	
	<logger name="wicket" level="info" />
	<logger name="cz.jmoz.projects.mmg" level="trace" />

	<logger name="org.springframework.transactions" level="debug" />
	
	<root level="INFO">
		<appender-ref ref="stdout" />	
	</root>

	

</configuration>